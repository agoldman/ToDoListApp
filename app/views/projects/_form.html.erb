<%= form_for(@project) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br/ >
  <%= f.label :description %>
  <%= f.text_field :description %> <br/ >

  <%= f.label :team %> <%= f.collection_select(:team_id, Team.all, :id, :name,
  {:selected => @team_id }
  )%>
  <br/ >
    <br/ >

  <%= f.submit "Make Project" %>
<% end %>